{
  "expo": {
    "name": "Smart Energy Meter",
    "slug": "energy-monitor-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-new.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon1.png",
        "backgroundColor": "#3F51B5"
      },
      "package": "com.yokka361.energymonitorapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1bedc0e4-4761-41d3-b58b-eb4f32cef79b"
      }
    },
    "splash": {
      "image": "./assets/images/splash-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#3F51B5"
    }
  }
}